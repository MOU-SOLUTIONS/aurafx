{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/AuraFX/browser",
  "rewrites": [
    {
      "source": "/((?!assets/|main.*\\.js|polyfills.*\\.js|runtime.*\\.js|styles.*\\.css|favicon\\.ico).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
